    value = "/media/yuqiong/DATA/rosproj/cb/ros_aiml/data"/>
  <node name="aiml_server" pkg="ros_aiml" type="aiml_server.py"
    output = "screen"/>
  </node>
  <include file="$(rospack find sound_play)/soundplay_node.launch">
  </include>
  <node name="aiml_tts" pkg="ros_aiml" type="aiml_tts_client.py"
